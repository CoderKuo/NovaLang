// 公共数据模型 —— 供其他文件引用

class User(val name: String, val age: Int) {
    fun greet(): String = "Hi, I'm " + name + " (" + age + ")"

    fun isAdult(): Boolean = age >= 18
}

class Product(val id: Int, val title: String, val price: Double) {
    fun display(): String = title + " - $" + price
}

interface Printable {
    fun prettyPrint(): String
}
